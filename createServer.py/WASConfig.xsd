    <xs:complexType name="MailProvider">
        <xs:annotation>
            <xs:documentation>Configuration for the JavaMail provider that supplies the JavaMail implementation used to
                create JavaMail sessions.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="MailSession">
                <xs:annotation>
                    <xs:documentation>Configurations for JavaMail support.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="Attributes">
                            <xs:complexType>
                                <xs:all>
                                    <xs:element minOccurs="0" name="J2EEResourceProperty">
                                        <xs:annotation>
                                            <xs:documentation>Custom Properties</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:attribute name="name" type="xs:string" use="optional"/>
                                            <xs:attribute name="value" type="xs:string" use="optional"/>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="mailFrom" type="xs:string">
                                        <xs:annotation>
                                            <xs:documentation>This field will appear to recipient as "From" or "ReplyTo"
                                                field.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:all>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="jndiName" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>The required JNDI name for the resource, including any naming subcontexts.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="mailTransportHost" type="xs:string" use="optional">
