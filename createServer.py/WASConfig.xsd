                                                <xs:element name="URLProvider" type="URLProvider"/>
                                                <xs:element name="WorkManagerProvider" type="WorkManagerProvider"/>
                                                <xs:element name="CacheProvider" type="CacheProvider"/>
                                                <xs:element name="NameBindings" type="NameBindings"/>
                                                <xs:element name="JDBCProvider" type="JDBCProvider"/>
                                                <xs:element name="JMSProvider" type="JMSProvider"/>
                                                <xs:element name="SIBMessagingEngine" type="SIBMessagingEngine"/>
                                                <xs:element name="VariableMap" type="VariableMap"/>
                                                <xs:element name="SharedLibraries" type="SharedLibraries"/>
                                            </xs:choice>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
