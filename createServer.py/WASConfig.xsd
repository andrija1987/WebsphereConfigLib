                                        <xs:sequence>
                                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                                <xs:element name="MailProvider" type="MailProvider"/>
                                                <xs:element name="URLProvider" type="URLProvider"/>
                                                <xs:element name="WorkManagerProvider" type="WorkManagerProvider"/>
